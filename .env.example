# Application
NODE_ENV=development
PORT=3000
SECRET_KEY=your-secret-key-change-this

# Database
DB_PATH=./data/crypto_signals.db

# Authentication
JWT_SECRET=your-jwt-secret-change-this
JWT_EXPIRATION=24h

# API Configuration
KUCOIN_API_URL=https://api.kucoin.com
